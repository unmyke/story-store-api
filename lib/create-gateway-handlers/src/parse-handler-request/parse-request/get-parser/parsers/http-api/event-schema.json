{
  "$schema": "http://json-schema.org/draft-07/schema",
  "$id": "http://dojorena.io/schemas/aws/events/http-api.json",
  "title": "AWS HTTP API event",
  "type": "object",
  "properties": {
    "version": {
      "type": "string",
      "pattern": "(^\\d+\\.)+\\d+$"
    },
    "routeKey": {
      "type": "string"
    },
    "rawPath": {
      "type": "string"
    }
  },
  "required": ["version", "routeKey", "rawPath"],
  "additionalProperties": true
}
